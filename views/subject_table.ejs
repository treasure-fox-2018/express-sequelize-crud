<html>
<head>
	<meta charset="utf-8">
	<meta http-equiv="X-UA-Compatible" content="IE=edge">
	<title>Subject Page</title>
	<link rel="stylesheet" href="">
</head>
<body>
	<h3>Subject Data</h3>
    <a href="/subject/add">add subject</a>
    <br><br>  
	<table border=1>
		<thead>
			<tr>
				<th>ID</th>
				<th>Subject Name</th>
			</tr>
		</thead>
		<tbody>
			<% subject.forEach((subject) => { %>
				<tr>
					<td><%=subject.id%></td>
					<td><%=subject.subject_name%></td>
					<td><a href="/subject/edit/<%= subject.id %>">Edit</a></td>
					<td><a href="/subject/delete/<%= subject.id%>" onclick="return confirm('Are you sure?')">Delete</td>
				</tr>
			<% }) %>
		</tbody>
    </table><br>
	<h3><a href="/">Back</a></h3>
</body>
</html>
