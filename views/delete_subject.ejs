    crossorigin="anonymous">

  <title>Delete Data Subject - Hacktiv8</title>
</head>

<body>

  <nav class="navbar navbar-expand-lg navbar-light fixed-top" style="background-color: #F57F17">
    <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarTogglerDemo03" aria-controls="navbarTogglerDemo03"
      aria-expanded="false" aria-label="Toggle navigation">
      <span class="navbar-toggler-icon"></span>
    </button>
    <a class="navbar-brand text-white" href="/">HACKTIV8</a>

    <div class="collapse navbar-collapse" id="navbarTogglerDemo03">
      <ul class="navbar-nav mr-auto mt-2 mt-lg-0">
        <li class="nav-item active">
          <a class="nav-link text-white" href="/">Home
            <span class="sr-only">(current)</span>
          </a>
        </li>
        <li class="nav-item">
          <a class="nav-link text-white" href="#">Login</a>
        </li>
        <li class="nav-item">
          <a class="nav-link text-white" href="#">Signup</a>
        </li>
      </ul>
      <form class="form-inline my-2 my-lg-0">
        <input class="form-control mr-sm-2" type="search" placeholder="Search" aria-label="Search">
        <button class="btn my-2 my-sm-0 text-white" type="submit" style="background-color: #FBC02D">Search</button>
      </form>
    </div>
  </nav>
  <br>
  <br>
  <br>
  <div class="container">
    <div class="row">
      <div class="col-lg-8 col-md-12 col-sm-12">
        <div class="card">
          <div class="card-header text-center" style="background-color: #FBC02D">
            <h4>Delete Data Subject</h4>
          </div>
          <div class="card-body">
            <form action="/subject/delete/<%= deleteSubject.id %>" method="POST">
              <div class="form-group">
                <label for="fistName">First Name</label>
                <input type="text" class="form-control" name="subject_name" value="<%= deleteSubject.subject_name %>">
              </div>
              <button type="button" class="btn btn-primary" data-toggle="modal" data-target="#modalSuccess">
                Delete Data
              </button>

              <!-- Modal -->
              <div class="modal fade" id="modalSuccess" tabindex="-1" role="dialog" aria-labelledby="exampleModalCenterTitle" aria-hidden="true">
                <div class="modal-dialog modal-dialog-centered" role="document">
                  <div class="modal-content">
                    <div class="modal-header">
                      <h5 class="modal-title" id="exampleModalCenterTitle">Notification Delete</h5>
                      <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">&times;</span>
                      </button>
                    </div>
                    <div class="modal-body">
                      Yakin sudah benar datanya ?
                    </div>
                    <div class="modal-footer">
                      <button type="button" class="btn btn-secondary" data-dismiss="modal">Cancel</button>
                      <!-- Button trigger modal -->
                      <button type="button" class="btn btn-primary" data-toggle="modal" data-target="#exampleModalCenter">
                        Save
                      </button>

                      <!-- Modal -->
                      <div class="modal fade" id="exampleModalCenter" tabindex="-1" role="dialog" aria-labelledby="exampleModalCenterTitle" aria-hidden="true">
                        <div class="modal-dialog modal-dialog-centered" role="document">
                          <div class="modal-content">
                            <div class="modal-header">
                              <h5 class="modal-title" id="exampleModalCenterTitle">Notification</h5>
                            </div>
                            <div class="modal-body">
                              Data Successfully Added
                            </div>
                            <div class="modal-footer">
                              <button type="submit" class="btn btn-primary">Ok</button>
                            </div>
                          </div>
                        </div>
                      </div>
                    </div>
                  </div>
                </div>
              </div>
            </form>
          </div>
        </div>
      </div>
      <div class="col-lg-4 col-md-12 col-sm-12">

        <div class="card text-center">
          <div class="card-header" style="background-color: #FBC02D">
            <h4>Data School</h4>
          </div>
          <div class="card-body">
            <h5 class="card-title">Data Student</h5>
            <a href="/student/add" class="btn btn-primary">Add Data</a>
            <br>
            <br>
            <h5>Data Teacher</h5>
            <a href="/teacher" class="btn btn-primary">Show Data</a>
            <a href="/teacher/add" class="btn btn-primary">Add Data</a>
            <br>
            <br>
            <h5>Data Subject</h5>
            <a href="/subject" class="btn btn-primary">Show Data</a>
            <a href="/subject/add" class="btn btn-primary">Add Data</a>
          </div>
        </div>
      </div>


    </div>
  </div>
  </div>

