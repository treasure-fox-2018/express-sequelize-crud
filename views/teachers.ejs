<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <title>Teacher List</title>

</head>
<body>
    <table>
        <tr>
          <th>id</th>
          <th>First Name</th>
          <th>Last Name</th>
          <th>Email</th>
        </tr>
        <% teachers.forEach(function(teacher) { %>
            <tr>
               <td> <%= teacher.id %></td>
               <td> <%= teacher.first_name %></td>
               <td> <%= teacher.last_name %></td>
               <td> <%= teacher.email %></td>
             </tr>

        <% }) %>
        
      </table>
</body>
</html>