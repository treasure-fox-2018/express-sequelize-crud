<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <title>Student List</title>

</head>
<body>
    <table>
        <tr>
          <th>id</th>
          <th>First Name</th>
          <th>Last Name</th>
        </tr>
        <% students.forEach(function(student) { %>
            <tr>
               <td> <%= student.id %></td>
               <td> <%= student.first_name %></td>
               <td> <%= student.last_name %></td>
               <td> <%= student.email %></td>
               <td><a href="/student/edit/<%= student.id %>">Edit</a></td>
			   <td><a href="/student/delete/<%= student.id%>" onclick="return confirm('Are you sure?')">Delete</td>
		
             </tr>

        <% }) %>
        
      </table>
</body>
</html>